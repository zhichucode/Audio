    "def nextSquare():\n",
    "    i = 1\n",
    "    while True:\n",
    "        yield i * i\n",
    "        i += 1\n",
    "        \n",
    "for num in nextSquare():\n",
    "    if num > 1000:\n",
    "         break\n",
    "    print(num)"
